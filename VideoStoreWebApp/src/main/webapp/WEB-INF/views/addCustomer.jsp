<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%><%@ page session="false"%><!DOCTYPE html><html><title>Video Store Add Customer Page</title><body>	<div>		<form:form action="createCustomer" id="createCustomerForm" commandName="newCustomer">			First Name:<form:input id="customerFirstName" path="firstName"/>			Last Name:<form:input id="customerLastName" path="lastName"/>			Regular Member:<form:radiobutton id="regMember" path="membershipType" value="REG"/>			VIP Member:<form:radiobutton id="vipMember" path="membershipType" value="VIP"/>			<input type="submit" id="submitNewCustomer" value="Submit"/>		</form:form>	</div></body></html>