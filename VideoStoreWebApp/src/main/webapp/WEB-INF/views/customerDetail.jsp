<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%><%@ page session="false"%><!DOCTYPE html><html><title>Video Store Customer Detail Page</title><body>	<div>			Customer Id:${customer.customerId}<br>			Name:${customer.name}<br>			Member Type:			<c:choose>				<c:when test="${memberType == 'VIP' }">VIP Customer</c:when>				<c:otherwise>Regular Joe</c:otherwise>			</c:choose>	</div></body></html>